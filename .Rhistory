regr.calc <- function(x){
if (cor.test(x[[1]], x[[2]])$p.value < 0.05) {
res <- lm(x[[1]] ~ x[[2]])
x$fit <- res$fitted.values
} else {
return("There is no sense in prediction")
}
}
regr.calc <- function(x){
if (cor.test(x[[1]], x[[2]])$p.value < 0.05) {
res <- lm(x[[1]] ~ x[[2]])
x$fit <- res$fitted.values
return(x)
} else {
return("There is no sense in prediction")
}
}
